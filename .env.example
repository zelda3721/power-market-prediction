# 数据库配置
DB_HOST=your_db_host
DB_PORT=your_db_port
DB_USER=xxx
DB_PASSWORD=abc
DB_NAME=data_collection

# 数据库连接池配置
DB_POOL_SIZE=10
DB_MAX_OVERFLOW=20
DB_POOL_TIMEOUT=30
DB_POOL_RECYCLE=3600

# 日志配置
LOG_LEVEL=INFO
LOG_FORMAT=json
LOG_DIR=logs

# 模型配置
MODEL_DIR=models
MODEL_VERSION=latest

# API配置
API_HOST=0.0.0.0
API_PORT=8000
API_WORKERS=4

# 缓存配置
CACHE_ENABLED=true
CACHE_TTL=3600

# 监控配置
METRICS_ENABLED=true
METRICS_PORT=9090

# 环境标识
ENVIRONMENT=development